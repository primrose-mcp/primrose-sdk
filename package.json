{
  "name": "primrose-mcp",
  "version": "0.1.1",
  "description": "Primrose SDK for MCP tool integration",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/primrose-mcp/primrose-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/primrose-mcp/primrose-sdk/issues"
  },
  "homepage": "https://github.com/primrose-mcp/primrose-sdk#readme",
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "mcp",
    "primrose",
    "tools",
    "llm",
    "sdk",
    "anthropic",
    "openai",
    "google",
    "amazon",
    "meta",
    "mistral"
  ],
  "author": "Primrose",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.5.1",
    "typescript": "^5.0.0",
    "vitest": "^2.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
